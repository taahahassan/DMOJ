M = int(input())
N = int(input())

rev = [0,1,8,11,88,111,101,181,818,888,808,1111,1001,1881,8888,8008,8118,11111,11011,10801, 11811, 18881,10001,18081,18181,10101]
double = [69,96,906,609,619,916,689,986,6699,9966,6969,9696, 1691, 1961,8698,8968,9006,6009,6119,9116,6889,9886,16091,19061,19161,19861,16191,16891]

count = 0
for i in range(M,N):
  if i in rev:
    count += 1
  if i in double:
    count += 1

print (count)
